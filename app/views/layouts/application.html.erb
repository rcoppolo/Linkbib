<!DOCTYPE html>
<html>
<head>
  <title>Linkbib</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>


	<%= render 'layouts/header' %>

	<div class="container" id="container_body">

		<% flash.each do |key, value| %>
		<div class="alert alert-<%= key %>"><%= value %></div>
		<% end %>
			<%= yield %>
	</div>


</body>
</html>